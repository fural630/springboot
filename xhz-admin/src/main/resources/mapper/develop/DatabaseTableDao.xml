<mapper namespace="com.xhz.web.module.develop.dao.DatabaseTableDao">


    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xhz.web.module.develop.entity.databasedoc.DatabaseTableDO">
