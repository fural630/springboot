<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

  <properties resource="mybaitis/generator.properties"/>

  <context id="DB2Tables" targetRuntime="MyBatis3">
  	
  	<!-- 生成的文件中不要添加注释 -->
  	<commentGenerator>
		<property name="suppressDate" value="true" />
		<property name="suppressAllComments" value="true" />
	</commentGenerator>
  
  
    <jdbcConnection driverClass="${driverClass}"
        connectionURL="${connectionURL}"
        userId="${username}"
        password="${password}">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.example.module.${package.name}.po" targetProject="src/main/java">
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="com.example.module.${package.name}.dao"  targetProject="src/main/java">
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.module.${package.name}.dao"  targetProject="src/main/java">
    </javaClientGenerator>

    <table tableName="system_user" domainObjectName="${entity.name}PO" mapperName="${entity.name}Mapper"
    	enableSelectByExample="false"
    	enableDeleteByExample="false"
    	enableCountByExample="false"
    	enableUpdateByExample="false"
    	selectByExampleQueryId="false">
    </table>

  </context>
</generatorConfiguration>